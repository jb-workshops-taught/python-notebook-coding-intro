    "def mult(a, b):\n",
    "    return a * b\n",
    "\n",
    "def sub(first, second):\n",
    "    return first - second\n",
    "\n",
    "def half(num):\n",
    "    return num // 2\n",
    "\n",
    "x = 50\n",
    "y = 10\n",
    "\n",
    "print(sub(half(mult(x, y)), x))"
   ]
